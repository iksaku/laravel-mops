{
    "name": "iksaku/laravel-mops",
    "description": "My Opinionated Scaffolding for Laravel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jorge Gonz√°lez",
            "email": "yo@jorgeglz.io"
        }
    ],
    "require": {
        "php": "^7.4",
        "ext-json": "*",
        "blade-ui-kit/blade-heroicons": "^0.3.0",
        "illuminate/console": "^8.0",
        "illuminate/support": "^8.0",
        "illuminate/view": "^8.0",
        "jenssegers/agent": "^2.6",
        "laravel/fortify": "^1.0",
        "laravel/sanctum": "^2.6.0",
        "livewire/livewire": "^2.3.0"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "iksaku\\Laravel\\Mops\\MopsServiceProvider"
            ]
        }
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "iksaku\\Laravel\\Mops\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
